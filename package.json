  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "start": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --import=tsx/esm --watch --no-warnings=ExperimentalWarning src/server.ts",
    "migrate:create": "tsx src/db/scripts/migrate-create.ts",
    "migrate:down": "tsx src/db/scripts/migrate-down.ts",
    "migrate:latest": "tsx src/db/scripts/migrate-latest.ts",
    "migrate:up": "tsx src/db/scripts/migrate-up.ts",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js . --max-warnings 0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jackfranklin/test-data-bot": "^2.1.0",
    "@jarsec/eslint-config": "5.2.0",
    "@jarsec/tsconfig": "2.1.0",
    "@types/node": "20.12.10",
    "@types/pg": "^8.11.6",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "@typespec/versioning": "^0.59.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.1",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "pino-pretty": "^11.0.0",
    "tsx": "4.19.1",
    "typescript": "5.5.4",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@fastify/autoload": "5.10.0",
    "@fastify/sensible": "5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/type-provider-typebox": "4.0.0",
    "@scalar/fastify-api-reference": "^1.22.31",
    "@sinclair/typebox": "0.32.35",
    "close-with-grace": "^1.3.0",
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "fastify": "4.27.0",
    "fastify-plugin": "4.5.1",
    "http-status-codes": "^2.3.0",
    "kysely": "^0.27.3",
    "neverthrow": "^6.2.1",
    "pg": "^8.11.5"
  }
