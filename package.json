{
  "name": "grid-governor",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "Jarrod Seccombe <jarrod.seccombe@icloud.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "start": "FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --import=tsimp/import --watch --no-warnings=ExperimentalWarning src/index.ts",
    "migrate:create": "tsimp src/db/scripts/migrate-create.ts && kysely-codegen",
    "migrate:down": "tsimp src/db/scripts/migrate-down.ts && kysely-codegen",
    "migrate:latest": "tsimp src/db/scripts/migrate-latest.ts && kysely-codegen",
    "migrate:up": "tsimp src/db/scripts/migrate-up.ts && kysely-codegen",
    "test": "ava",
    "lint": "eslint --ext .js . --max-warnings 0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@jarsec/eslint-config": "5.1.0",
    "@jarsec/tsconfig": "2.1.0",
    "@types/node": "20.12.10",
    "@types/pg": "^8.11.6",
    "ava": "6.1.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.56.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "pino-pretty": "^11.0.0",
    "tsimp": "2.0.11",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@fastify/autoload": "5.8.2",
    "@fastify/sensible": "5.6.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "4.0.0",
    "@sinclair/typebox": "0.32.29",
    "dotenv": "16.4.5",
    "envalid": "8.0.0",
    "fastify": "4.27.0",
    "fastify-plugin": "4.5.1",
    "kysely": "^0.27.3",
    "kysely-codegen": "^0.15.0",
    "mysql2": "^3.9.7",
    "pg": "^8.11.5"
  }
}
